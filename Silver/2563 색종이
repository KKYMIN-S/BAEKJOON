#include <iostream>
using namespace std;

int main()
{
	int n, x, y;
	int sum = 0;
	int array[101][101];

	cin >> n;

	for (int i = 0; i < n; i++)
	{
		cin >> x >> y;

		for (int i = x; i < x + 10; i++)
		{
			for (int j = y; j < y + 10; j++)
			{
				if (array[i][j] != 1)
				{
					array[i][j] = 1;
					sum++;
				}
			}
		}
	}
	cout << sum;
}
