import sys
input = sys.stdin.readline

def solution():
    n = int(input())
    L = []
    for i in range(n):
        L.append(list(map(int, input().split())))
    L.sort(key=lambda x: (x[1], x[0])) # y좌표를 기준으로 먼저 정렬하고, y좌표가 같으면 x좌표를 기준으로 정렬
    for i in range(n):
        print(L[i][0], L[i][1])

solution()
