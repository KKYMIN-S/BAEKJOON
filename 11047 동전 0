def solution(): # 그리디 알고리즘
    n, k = map(int, input().split())
    coins = []
    count = 0

    for _ in range(n):
        coin = int(input())
        if coin <= k:
            coins.append(coin)
    coins.reverse() # 역순
    for i in coins:
        count += k // i
        k = k % i

    print(count)

solution()
