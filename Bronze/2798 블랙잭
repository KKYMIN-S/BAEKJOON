def solution(N, M):
    max = 0

    for i in range(N):
        for j in range(i+1, N):
            for k in range(j+1, N):
                sum = L[i] + L[j] + L[k]
                if sum > M:
                    continue
                elif sum > max:
                    max = sum
    return max

n, m = map(int, input().split())
L = list(map(int, input().split()))
result = solution(n, m)
print(result)
