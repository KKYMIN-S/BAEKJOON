#include <iostream>
using namespace std;

int main()
{
	int x, y, z;

	cin >> x >> y >> z;
	
	int max = x;

	if (x == y && y == z) {
		cout << 10000 + x * 1000;
	}
	else if ((x == y) && (y != z)) {
		cout << 1000 + x * 100;
	}
	else if ((y == z) && (x != y)) {
		cout << 1000 + y * 100;
	}
	else if ((z == x) && (y != z)) {
		cout << 1000 + z * 100;
	}
	else {
		if (y > max) {
			max = y;
		}
		if (z > max) {
			max = z;
		}
		cout << max * 100;
	}
}
