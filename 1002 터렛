#include <iostream>
#include <cmath>
using namespace std;

int main()
{
	int x1 = 0, x2 = 0, y1 = 0, y2 = 0, r1 = 0, r2 = 0;
	double distance = 0.0;
	int test_num;

	cin >> test_num;
	int* result = new int[test_num];
	
	for (int i = 0; i < test_num; i++)
	{
		cin >> x1 >> y1 >> r1 >> x2 >> y2 >> r2;
		distance = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));

		if ((x1 == x2) && (y1 == y2)) // 원 중심 같은 경우
		{
			if ((r1 == r2))
				result[i] = -1;
			else
				result[i] = 0;
		}
		else // 원 중심 다른 경우
		{
			if ((r1 + r2 < distance) || (distance < abs(r2 - r1)))
				result[i] = 0;
			else if ((r1 + r2 == distance) || (abs(r1 - r2) == distance))
				result[i] = 1;
			else if ((r1 + r2 > distance) && (distance > abs(r2 - r1)))
				result[i] = 2;
            else
                result[i] = 0;
		}
	}

	for (int i = 0; i < test_num; i++)
	{
		cout << result[i] << endl;
	} 
}
